                    <canal-de-prestacao id="1826" tipo="web-agendar">
                        <descricao>http://www.detran.df.gov.br/agendamento.html</descricao>
                    </canal-de-prestacao>
                </default>
            </canais-de-prestacao>
        </etapa>
    </etapas>
